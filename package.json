{
  "private": true,
  "name": "nl.kabisa.demo",
  "version": "0.0.1",
  "scripts": {
    "analyze-bundle": "cross-env ANALYZE_BUNDLE=true NODE_ENV=production webpack",
    "build": "cross-env GIT_COMMIT=$(git rev-parse HEAD) NODE_ENV=production webpack",
    "start": "node script/dev-server.js",
    "test": "yarn lint && yarn test:unit && yarn test:e2e --env ci",
    "test:unit": "cross-env NODE_ENV=test karma start config/karma.js --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start config/karma.js --watch",
    "test:e2e": "cross-env NODE_ENV=test SERVER_PORT=9091 script/test-e2e.js",
    "lint": "yarn eslint && yarn stylelint && yarn tslint",
    "eslint": "eslint src/ test/",
    "stylelint": "stylelint 'src/**/*.scss'",
    "tslint": "tslint --project . --config config/tslint.json ./**/*.ts{,x}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "prefix-commit-message $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint",
    "*.scss": "stylelint",
    "*.{ts,tsx}": "tslint"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "i18n-js": "^3.0.3",
    "react": "^16.7.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/chai": "^4.1.7",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.1.16",
    "@types/fetch-mock": "^7.2.3",
    "@types/i18n-js": "^3.0.1",
    "@types/mocha": "^5.2.5",
    "@types/react": "^16.8.1",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/sinon": "^7.0.5",
    "@types/sinon-chai": "^3.2.2",
    "autoprefixer": "^9.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^5.0.1",
    "chai": "^4.1.2",
    "chromedriver": "^2.38.3",
    "clean-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "cssnano": "^4.1.4",
    "deep-equal": "^1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.2.0",
    "eslint-plugin-compat": "^2.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.6.1",
    "fetch-mock": "^7.0.7",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "json-loader": "^0.5.4",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^4.0.0-rc.3",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.2",
    "nightwatch": "^1.0.18",
    "node-sass": "^4.9.0",
    "postcss-loader": "^3.0.0",
    "prefix-commit-message": "^1.0.1",
    "prettier": "^1.13.0",
    "react-dom": "^16.7.0",
    "sass-loader": "^7.0.1",
    "selenium-server-standalone-jar": "^3.11.0",
    "sinon": "^7.0.0",
    "sinon-chai": "^3.1.0",
    "source-map-loader": "^0.2.1",
